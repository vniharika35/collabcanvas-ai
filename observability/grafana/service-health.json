{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Span throughput aggregated from OpenTelemetry spanmetrics.",
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "mappings": [],
          "unit": "1/s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum(rate(calls{service_name=~\"$service\"}[5m])) by (service_name)",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ],
      "title": "Span Throughput",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "P95 latency derived from spanmetrics histogram buckets (milliseconds).",
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "mappings": [],
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(latency_bucket{service_name=~\"$service\"}[5m])) by (le, service_name)) / 1000000",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ],
      "title": "P95 Latency",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Percentage of spans with non-OK status codes.",
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "mappings": [],
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "id": 3,
      "options": {
        "legend": { "calcs": [], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "100 * sum(rate(calls{status_code!=\"STATUS_CODE_UNSET\", status_code!=\"STATUS_CODE_OK\", service_name=~\"$service\"}[5m])) by (service_name) / sum(rate(calls{service_name=~\"$service\"}[5m])) by (service_name)",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["collabcanvas", "otel"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "label": "Prometheus",
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "current": { "text": "All", "value": "/.*/" },
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "definition": "label_values(calls, service_name)",
        "hide": 0,
        "includeAll": true,
        "label": "Service",
        "multi": true,
        "name": "service",
        "options": [],
        "query": "label_values(calls, service_name)",
        "refresh": 1,
        "regex": "",
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "browser",
  "title": "CollabCanvas Service Health",
  "uid": null,
  "version": 1
}
