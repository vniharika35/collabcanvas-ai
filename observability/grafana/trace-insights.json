{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Operations ranked by request rate and p95 latency (ms).",
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "footer": { "enablePagination": true, "fields": "", "reducer": ["sum"] },
        "showHeader": true,
        "sortBy": [ { "desc": true, "displayName": "latency_ms" } ]
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(latency_bucket{service_name=~\"$service\", name=~\"$operation\"}[5m])) by (le, service_name, name)) / 1000000",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        },
        {
          "expr": "sum(rate(calls{service_name=~\"$service\", name=~\"$operation\"}[5m])) by (service_name, name)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "B"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "Value": "latency_ms",
              "Value #B": "requests_per_s",
              "name": "operation",
              "service_name": "service"
            }
          }
        }
      ],
      "title": "Operation Hotspots",
      "type": "table"
    },
    {
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "description": "TraceQL search helper scoped by service selector.",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 10 },
      "id": 2,
      "targets": [
        {
          "query": "{service.name=\\\"$service_exact\\\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "title": "TraceQL Search",
      "type": "traceql-search"
    }
  ],
  "refresh": "1m",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["collabcanvas", "traces"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "refresh": 1
      },
      {
        "name": "tempo",
        "type": "datasource",
        "label": "Tempo",
        "query": "tempo",
        "refresh": 1
      },
      {
        "current": { "text": "All", "value": "/.*/" },
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "definition": "label_values(calls, service_name)",
        "includeAll": true,
        "label": "Service (regex)",
        "multi": true,
        "name": "service",
        "query": "label_values(calls, service_name)",
        "refresh": 1,
        "type": "query"
      },
      {
        "current": { "text": "All", "value": "/.*/" },
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "definition": "label_values(calls, name)",
        "includeAll": true,
        "label": "Operation (regex)",
        "multi": true,
        "name": "operation",
        "query": "label_values(calls, name)",
        "refresh": 1,
        "type": "query"
      },
      {
        "current": { "text": "collabcanvas-web", "value": "collabcanvas-web" },
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "definition": "label_values(calls, service_name)",
        "hide": 0,
        "includeAll": false,
        "label": "Service (exact)",
        "multi": false,
        "name": "service_exact",
        "query": "label_values(calls, service_name)",
        "refresh": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-30m", "to": "now" },
  "timezone": "browser",
  "title": "CollabCanvas Trace Insights",
  "version": 1
}
